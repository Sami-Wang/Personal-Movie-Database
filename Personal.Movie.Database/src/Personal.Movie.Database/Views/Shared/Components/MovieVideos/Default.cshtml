@model List<Personal.Movie.Database.Model.MovieModel.MovieVideo>

<div class="row"> 
    @if (Model != null && Model.Count != 0)
    {
        <div class="row">
            <div class="col-lg-12">
                <h3 class="page-header">Videos</h3>
                <div class="row">
                    @for (int i = 0; i < Model.Count; i++)
                    {
                        <div class="col-sm-3 col-xs-6">
                            <iframe class="img-responsive portfolio-item" src=@Model[i].videoURL frameborder="0" allowfullscreen></iframe>
                        </div>
                    }
                </div>
            </div>      
        </div>
    }
</div>